{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "composite": true,
    "module": "ES2020",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~/vendor/*": [
        "./vendor/*"
      ],
      "~/wails/*": [
        "./wailsjs/*"
      ],
      "~/*": [
        "./src/*"
      ]
    },
    "types": [
      "node"
    ]
  },
  "include": [
    "vite.config.ts"
  ]
}